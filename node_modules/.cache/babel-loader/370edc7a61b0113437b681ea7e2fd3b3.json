A,CAAA,MAAA,EAAA,GAAA,CAAA,IAAA,CAAA;AACA,eAAA,MAAA,CAAA,QAAA,CAAA,SAAA,EAAA,GAAA,CAAA,IAAA;AACA,UAAA,UAAA,CAAA,MAAA;AACA,iBAAA,OAAA,CAAA,IAAA,CAAA,GAAA;AACA,WAFA,EAEA,IAFA,CAAA;AAGA;AACA,OAZA;AAcA,KAjBA;;AAkBA,IAAA,OAAA,CAAA,KAAA,EAAA;AACA,MAAA,KAAA,CAAA,cAAA,GADA,CAEA;;AACA,WAAA,IAAA,CAAA,KAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,EAAA;AACA,WAAA,IAAA,CAAA,IAAA,GAAA,IAAA;AACA,WAAA,IAAA,CAAA,OAAA,GAAA,EAAA,CANA,CAOA;;AACA,WAAA,IAAA,GAAA,KAAA;AACA,WAAA,SAAA,CAAA,MAAA;AACA,aAAA,IAAA,GAAA,IAAA;AACA,OAFA;AAGA,KA9BA;;AA+BA,IAAA,gBAAA,CAAA,gBAAA,EAAA;AACA,WAAA,gBAAA,GAAA,gBAAA;AACA,KAjCA;;AAkCA,IAAA,SAAA,GAAA;AACA,WAAA,gBAAA,GAAA,KAAA,WAAA;AACA;;AApCA;AAfA,CAAA","sourcesContent":["<template>\n  <div>\n    <b-alert\n      :show=\"dismissCountDown\"\n      dismissible\n      variant=\"success\"\n      @dismissed=\"dismissCountDown = 0\"\n      @dismiss-count-down=\"countDownChanged\"\n    >\n      <p>Login successful. Please wait.</p>\n      <b-progress\n        variant=\"success\"\n        :max=\"dismissSecs\"\n        :value=\"dismissCountDown\"\n        height=\"4px\"\n      ></b-progress>\n    </b-alert>\n\n    <b-form @submit=\"onSubmit\" @reset=\"onReset\" v-if=\"true\">\n      <b-form-group id=\"input-group-1\" label-for=\"input-1\">\n        <b-form-input\n          id=\"input-1\"\n          v-model=\"form.email\"\n          type=\"email\"\n          placeholder=\"Email\"\n          required\n        ></b-form-input>\n      </b-form-group>\n      <b-form-group id=\"input-group-2\" label-for=\"input-2\">\n        <b-form-input\n          id=\"input-2\"\n          type=\"password\"\n          v-model=\"form.password\"\n          placeholder=\"Password\"\n          required\n        ></b-form-input>\n      </b-form-group>\n      <div class=\"d-flex justify-content-center align-items-center\">\n        <b-button type=\"submit\" variant=\"primary\" class=\"mr-2\">Submit</b-button>\n      </div>\n    </b-form>\n  </div>\n</template>\n\n<script>\nimport { login } from \"@/services/userAuth\";\nimport { saveToLocal, removeFromLocal } from \"@/utils/localStorage\";\nexport default {\n  name: \"Login\",\n  data() {\n    return {\n      form: {\n        email: \"\",\n        password: \"\",\n      },\n\n      show: true,\n      dismissSecs: 5,\n      dismissCountDown: 0,\n      showDismissibleAlert: false,\n    };\n  },\n  methods: {\n    onSubmit(event) {\n      event.preventDefault();\n      login({ email: this.form.email, password: this.form.password }).then(\n        (res) => {\n          console.log('res', res);\n          if (res.status === 200) {\n            this.showAlert();\n            removeFromLocal(\"user\");\n            saveToLocal(\"user\", res.data);\n            this.$store.dispatch(\"setUser\", res.data);\n            setTimeout(() => {\n              this.$router.push(\"/\");\n            }, 5000);\n          }\n        }\n      );\n    },\n    onReset(event) {\n      event.preventDefault();\n      // Reset our form values\n      this.form.email = \"\";\n      this.form.name = \"\";\n      this.form.food = null;\n      this.form.checked = [];\n      // Trick to reset/clear native browser form validation state\n      this.show = false;\n      this.$nextTick(() => {\n        this.show = true;\n      });\n    },\n    countDownChanged(dismissCountDown) {\n      this.dismissCountDown = dismissCountDown;\n    },\n    showAlert() {\n      this.dismissCountDown = this.dismissSecs;\n    },\n  },\n};\n</script>\n<style>\n</style>\n"],"sourceRoot":"src/components/Login"}]}