b-form-group>\n      <b-button variant=\"primary\" size=\"sm\">Publish</b-button>\n    </b-form>\n</div>\n</template>\n\n<script>\nexport default {\n  name: 'PostInput',\n  data () {\n    return {\n      form: {\n        email: '',\n        name: '',\n        food: null,\n        checked: []\n      },\n      foods: [{ text: 'Select One', value: null }, 'Carrots', 'Beans', 'Tomatoes', 'Corn'],\n      show: true\n    }\n  },\n  methods: {\n    onSubmit (event) {\n      event.preventDefault()\n      alert(JSON.stringify(this.form))\n    },\n    onReset (event) {\n      event.preventDefault()\n      // Reset our form values\n      this.form.email = ''\n      this.form.name = ''\n      this.form.food = null\n      this.form.checked = []\n      // Trick to reset/clear native browser form validation state\n      this.show = false\n      this.$nextTick(() => {\n        this.show = true\n      })\n    }\n  }\n}\n</script>\n\n<style scoped>\n.form-wrapper{\n  display: flex;\n  margin-top: 1rem;\n  margin-bottom: 1rem;\n  padding: 1rem;\n  border: 1px solid rgb(199, 199, 199);\n  border-style: none none solid none;\n}\n.form-wrapper .post-form{\n  flex: 0.9;\n  margin-left: 0.5rem;\n\n}\n.form-input{\n  position: relative;\n  height: 5rem;\n}\n.form-input::placeholder{\n  position: absolute;\n}\n\n</style>\n"]}]}