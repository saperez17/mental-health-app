      </b-form-group>\n      <b-button type=\"submit\" variant=\"primary\" size=\"sm\">Publish</b-button>\n    </b-form>\n  </div>\n</template>\n\n<script>\nimport store from \"@/store\";\n\nexport default {\n  name: \"PostInput\",\n  data() {\n    return {\n      post: {\n        content: \"\",\n        sharedState: store.state.posts,\n      },\n    };\n  },\n  methods: {\n    onSubmit(event) {\n      event.preventDefault();\n      // alert(JSON.stringify(this.form))\n      console.log(\"submiting post\");\n      store.addPostAction();\n    },\n    onReset(event) {\n      event.preventDefault();\n      // Reset our form values\n      this.post.content = \"\";\n      this.$nextTick(() => {\n        this.show = true;\n      });\n    },\n  },\n  created() {\n    this.$nextTick(function () {\n     console.log(store.fetchPostsAction());\n    });\n  },\n};\n</script>\n\n<style scoped>\n.form-wrapper {\n  display: flex;\n  margin-top: 1rem;\n  margin-bottom: 1rem;\n  padding: 1rem;\n  border: 1px solid rgb(199, 199, 199);\n  border-style: none none solid none;\n}\n.form-wrapper .post-form {\n  flex: 0.9;\n  margin-left: 0.5rem;\n}\n.form-input {\n  position: relative;\n  height: 5rem;\n}\n.form-input::placeholder {\n  position: absolute;\n}\n</style>\n"]}]}