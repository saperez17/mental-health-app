\n        console.log('res', res);\n        this.userObj = res.data\n      })\n    }\n    console.log('userObj', this.userObj);\n  },\n  methods: {\n    reducer(prev, curr) {\n      return prev.likes.length + curr;\n    },\n  },\n};\n</script>\n\n<style scoped>\n.wrapper {\n  height: 70vh;\n  display: flex;\n  flex-direction: column;\n  justify-content: center;\n  align-items: center;\n}\n</style>"]}]}